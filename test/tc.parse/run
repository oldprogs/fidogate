#!/bin/sh

T=../../src/common/testparse


t1() {
  echo "t1         : $1"
  $T "$1"
}

t2() {
  echo "t2         : $1 $2"
  $T "$1" "$2"
}

echo "tc: running testparse"

t1 "1:2/3.4"
t1 "1:2/3"
t1 "1:2/255"
t1 "1:2"
t1 "2/3.4"
t1 "3.4"
t1 "3"
t1 "*"
t1 "all"
t1 "World"
t1 "*:*"
t1 "*:*/*"
t1 "all:*/*.all"
t1 "2:2/3.*"
t1 "3:2/*"
t1 "4:*"
t1 "5:*/3"
t1 "6:*/4.255"

t2 "1:2/3.4" ".8"
t2 "2:2/3.4" ".99"
t2 "3:2/3.4" "0.8"
t2 "4:2/3.4" "98/99.0"
t2 "5:2/3.4" "99/0"
t2 "242:2/3.4" "4900/0"
t2 "1:2/3" ".*"
t2 "2:2/3" "*/*"
t2 "4:3/2.1" "1:*"

