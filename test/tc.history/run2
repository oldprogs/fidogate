./rndmsgid -n  25000 | time -p $T
echo "     + 25000  225000"
./rndmsgid -n  25000 | time -p $T
echo "     + 25000  250000"
./rndmsgid -n  25000 | time -p $T
echo "     + 25000  275000"
./rndmsgid -n  25000 | time -p $T
echo "     + 25000  300000"
./rndmsgid -n  25000 | time -p $T


