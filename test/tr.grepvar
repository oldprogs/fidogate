#!/usr/bin/perl
#
# Remove strings with variable contents (date, time, version number,
# stats) from test case output
#

while(<>) {
    next if( /^ *Date:/ );
    next if( /^now=/ );
    next if( /^\^AVia / );
    next if( /msgs\/s/ );
    next if( /Kbyte\/s/ );
    next if( /^Free disk space/ );
    next if( /unique.*lock/ );
    next if( /^From / );
    next if( /^\s*id / );

    s/FIDOGATE \d+\.\d+\.\d+/FIDOGATE x.x.x/g;

    print;
}
