#!/bin/sh
#
# $Id: mk.verify,v 4.4 1998/04/11 16:31:05 mj Exp $
#
# Create run.verify
#

VERIFY=run.verify

./run 2>&1 \
    | grep -v '^ *Date:' \
    | grep -v '^now=' \
    | grep -v '^\^AVia ' \
    | grep -v 'msgs/s' \
    | grep -v 'Kbyte/s' \
    | grep -v 'Internet -> FIDO gateway / FIDOGATE' \
    > $VERIFY
